@model Restaurant

@{
    ViewBag.Title = Model.Name;
}

<div class="card">
    <div class="card-body">
        <h1 class="card-title">@Model.Name</h1>
        <h5>ID: @Model.Id</h5>
        <h5>Cuisine: @Model.Cuisine</h5>
        <a asp-action="Index" asp-controller="Home" class="btn btn-primary">Back</a>
    </div>
</div>